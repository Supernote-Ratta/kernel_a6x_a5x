		status = "okay";
		compatible = "ti,tps65185";
		reg = <0x68>;
		pinctrl-names = "default";
		pinctrl-0 = <&tps65185_gpio>;
		wakeup_pin = <&gpio0 RK_PA1 GPIO_ACTIVE_HIGH>;
